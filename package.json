{
  "version": "0.1.5",
  "name": "nhm-ui",
  "author": "Daniel Markbreiter",
  "module": "dist/nhm-ui.esm.js",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "http://naturalhistorymuseumofla.github.io/nhm-ui",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -o storybook-static"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/tdsx-example.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tdsx-example.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@babel/preset-env": "^7.22.6",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-webpack5": "^7.0.26",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.6",
    "storybook": "^7.0.26",
    "tailwindcss": "^3.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "clsx": "^1.2.1",
    "istanbul-lib-coverage": "^3.2.0",
    "module-alias": "^2.2.3",
    "stylus": "^0.59.0"
  }
}
